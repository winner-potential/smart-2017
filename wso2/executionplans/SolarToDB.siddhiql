@Plan:name('SolarToDB')

@Plan:trace('false')

@Import('SolarIn:1.0.0')
define stream solarin (time long, value double, station string, id string);

@Export('SolarOut:1.0.0')
define stream solarout (name string, time long, value double, station string, id string);

from solarin
select 'pvenergy' as name, time, value, station, id
insert into solarout;